@using Humanizer;
@functions{
    public string dasherize(string dashed)
    {
        return dashed.Underscore().Dasherize();
    }
    public string classify(string classified)
    {
        return classified.Pascalize();
    }
}
/*
 * Public API Surface of @dasherize(Model.name)
 */

export * from './lib/@(dasherize(Model.name)).service';
export * from './lib/@(dasherize(Model.name)).component';
export * from './lib/@(dasherize(Model.name)).module';
